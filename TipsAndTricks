// Virtual Host setup

C:\xampp\apache\conf\extra\httpd-vhosts.conf
C:\Windows\System32\drivers\etc\hosts

https://www.moesif.com/blog/technical/cors/Authoritative-Guide-to-CORS-Cross-Origin-Resource-Sharing-for-REST-APIs/
<VirtualHost *:80>
  DocumentRoot "C:\xampp\htdocs\bls-api-server\public"
  Header always add Access-Control-Allow-Origin "http://localhost:4200"
  Header always add Access-Control-Allow-Headers "origin, content-type, Authorization"
  Header always add Access-Control-Allow-Methods "PUT, GET, POST, DELETE, OPTIONS"
  ServerName blsapi.test
</VirtualHost>
Header always add Access-Control-Allow-Headers 'x-requested-with, Content-Type, origin, authorization, accept, client-security-token'

